<div class="" id="game">

</div>

<script>
    import Game from '../game/game';
    import AtaObject from '../game/objects/ataObject';
    import LevelParser from '../game/levelParser';
    import type Level from '../game/level/level';

    const game = new Game({
        element: document.getElementById("game")!
    })

    game.addObject(new AtaObject({}))

    const levelLoader = new LevelParser(game)
    fetch('/game/map.json')
        .then(r => r.json() as unknown as Level)
        .then(l => levelLoader.load(l))
</script>